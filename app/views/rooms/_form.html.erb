<%= form_with(model: room, local: true) do |form| %>
  <% if room.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(room.errors.count, "error") %> prohibited this room from being saved:</h2>

      <ul>
      <% room.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :room %>
    <%= form.text_field :room, id: :room_room %>
  </div>

  <div class="field">
    <%= form.label :owner %>
    <%= form.text_field :owner, id: :room_owner, value: session[:current_user_id] %>
  </div>
  <div class="field">
    <%= form.label :rando %>
    <%= form.text_field :rando, id: :room_rando, value: @string %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
